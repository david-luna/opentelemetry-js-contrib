{
  "name": "@opentelemetry/instrumentation-undici",
  "version": "0.14.0",
  "description": "OpenTelemetry instrumentation for `undici` http client and Node.js fetch()",
  "main": "build/src/index.js",
  "module": "build/esm/index.js",
  "types": "build/src/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib.git",
    "directory": "packages/instrumentation-undici"
  },
  "scripts": {
    "clean": "tsc --build --clean tsconfig.json tsconfig.cjs.json",
    "compile": "tsc --build tsconfig.json tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > build/cjs/package.json",
    "compile:with-dependencies": "nx run-many -t compile -p @opentelemetry/instrumentation-undici",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run compile",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "test": "nyc mocha --loader=ts-node/esm test/**/*.test.ts",
    "test-all-versions": "tav",
    "version:update": "node ../../scripts/version-update.js"
  },
  "keywords": [
    "opentelemetry",
    "fetch",
    "undici",
    "nodejs",
    "tracing",
    "instrumentation"
  ],
  "author": "OpenTelemetry Authors",
  "license": "Apache-2.0",
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "exports": {
    "import": {
      "types": "./build/src/index.d.ts",
      "default": "./build/src/index.js"
    },
    "require": {
      "types": "./build/cjs/index.d.ts",
      "default": "./build/cjs/index.js"
    }
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "build/cjs/**/*.js",
    "build/cjs/**/*.js.map",
    "build/cjs/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-metrics": "^2.0.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@types/mocha": "10.0.10",
    "@types/node": "18.18.14",
    "nyc": "17.1.0",
    "rimraf": "5.0.10",
    "semver": "^7.6.0",
    "test-all-versions": "6.1.0",
    "typescript": "5.0.4",
    "undici": "6.21.3"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.7.0"
  },
  "dependencies": {
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/instrumentation": "^0.203.0"
  },
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/packages/instrumentation-undici#readme",
  "sideEffects": false
}
