{
  "name": "@opentelemetry/instrumentation-undici",
  "version": "0.14.0",
  "description": "OpenTelemetry instrumentation for `undici` http client and Node.js fetch()",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib.git",
    "directory": "packages/instrumentation-undici"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "setup:dev": "nx run-many -t compile -p @opentelemetry/instrumentation-undici",
    "compile:old": "tsc -p .",
    "compile": "tsup-node src/index.ts --clean --format cjs,esm --no-splitting --sourcemap --dts",
    "test": "nyc mocha test/**/*.test.ts",
    "test-all-versions": "tav",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "clean": "rimraf build/*",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "watch": "tsc -w",
    "version:update": "node ../../scripts/version-update.js"
  },
  "keywords": [
    "opentelemetry",
    "fetch",
    "undici",
    "nodejs",
    "tracing",
    "instrumentation"
  ],
  "author": "OpenTelemetry Authors",
  "license": "Apache-2.0",
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.mjs",
    "dist/**/*.mjs.map",
    "dist/**/*.d.mts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-metrics": "^2.0.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@types/mocha": "10.0.10",
    "@types/node": "18.18.14",
    "nyc": "17.1.0",
    "rimraf": "5.0.10",
    "semver": "^7.6.0",
    "test-all-versions": "6.1.0",
    "tsup": "^8.5.0",
    "typescript": "5.0.4",
    "undici": "6.21.3"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.7.0"
  },
  "dependencies": {
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/instrumentation": "^0.203.0"
  },
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/packages/instrumentation-undici#readme",
  "sideEffects": false
}
